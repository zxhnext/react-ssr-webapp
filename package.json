{
  "name": "react-ssr-webapp",
  "version": "1.0.0",
  "description": "react全栈加服务端渲染开发webapp",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf dist",
    "lint": "eslint --ext .jsx --ext .js client/",
    "lint-fix": "eslint --fix --ext .jsx --ext .js client/ build/ server/",
    "precommit": "npm run lint-fix && npm run lint",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.client.config.js",
    "dev:server": "nodemon server/server.js",
    "dev": "npm-run-all --parallel dev:**",
    "start": "cross-env NODE_ENV=production node server/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "http-proxy-middleware": "^0.20.0",
    "memory-fs": "^0.5.0",
    "mobx": "^5.14.2",
    "mobx-react": "^6.1.4",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-stage-0": "^7.0.0",
    "airbnb": "0.0.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.12.15",
    "rimraf": "^3.0.0",
    "standard": "^14.3.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
